  "scripts": {
    "dev": "NODE_ENV=development node-dev server | ./node_modules/.bin/bunyan -o short",
    "build": "./node_modules/.bin/babel-node node_modules/webpack/bin/webpack.js --config builder/webpack.production.config.js --production",
    "stage": "PORT=6101 NODE_ENV=stage node-dev server",
    "deploy": "npm run build & ./deploy.sh",
    "test": "NODE_CONFIG_DIR=../config NODE_ENV=development ava --require babel-register",
    "watch:test": "npm test -- --watch",
    "seed": "./node_modules/.bin/babel-node scripts/seed",
    "postinstall": "gulp build --gulpfile ./client/semantic/gulpfile.js"
  },
